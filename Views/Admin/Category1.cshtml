@using Shopaholics.Models

@{
    ViewBag.Title = "Category1";
    Layout = "~/Views/Shared/Layout1.cshtml";
}

<div id="section">
@foreach (var i in Model)
{

    <div class="col-md-4">
        <div class="item">
                <img src="..@i.Image" alt="@i.Description" style="width:200px;height:120px">
          
        </div>
        <div>
            <p class="navbar-text">Rs @i.Price | Stock: @i.Stock</p>
        </div>
        <br />
        <br />
    </div>

}
    </div>

